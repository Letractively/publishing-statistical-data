  rdfs:isDefinedBy <http://sdmx.org/wp-content/uploads/2009/01/01_sdmx_cog_annex_1_cdc_2009.pdf> ;
  rdfs:subPropertyOf sdmx:qualityMgmnt .

sdmx:qualityDoc a rdf:Property ;
